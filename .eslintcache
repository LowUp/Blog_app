s/post.action\";\r\n\r\nconst News = () =>{\r\n\r\n    const [newsData, setNewsData] = useState([]);\r\n    const [author, setAuthor] = useState(\"\");\r\n    const [message, setMessage] = useState(\"\");\r\n    const [error, setError] = useState(false);\r\n\r\n    const data = useSelector((state) => state.postReducer);\r\n\r\n    const dispatch = useDispatch();\r\n\r\n      const getData = () =>\r\n      {  \r\n          const dataArray = Object.keys(data).map((i) => data[i]);\r\n          setNewsData(dataArray);  \r\n      } \r\n\r\n    const handleSubmit = async (event) =>{\r\n        \r\n        const information = {\r\n            author,\r\n            content: message,\r\n            date: Date.now(),\r\n        };\r\n        \r\n        event.preventDefault();\r\n        \r\n        if(message.length < 20)\r\n        {\r\n            setError(true);\r\n\r\n        }\r\n        else{\r\n\r\n           await dispatch(addPosts(information));\r\n            dispatch(getPosts());\r\n            setAuthor(\"\");\r\n            setMessage(\"\");\r\n            getData();\r\n            setError(false);\r\n\r\n            }\r\n    };\r\n\r\n     useEffect(() =>{\r\n\r\n         getData();\r\n        //console.log(newsData);\r\n\r\n     }, [data])\r\n\r\n    return(\r\n        <div className=\"news-container\">\r\n            <h1>Blog</h1>\r\n\r\n            <form onSubmit={(e) => handleSubmit(e)} >\r\n\r\n                <input \r\n                type=\"text\" \r\n                placeholder=\"Name\" \r\n                value={author} \r\n                onChange={(e) => setAuthor(e.target.value)} />\r\n                \r\n                <textarea \r\n                style={error ? {border : \"1px solid red\"} : {border : \"1px solid #31dafb\"}}\r\n                placeholder=\"Message\" \r\n                value={message} onChange={(e) => setMessage(e.target.value)}/>\r\n                \r\n                { error && <p>Needs at least 20 characters</p>}\r\n\r\n                <input \r\n                type=\"submit\" \r\n                value=\"Send\"/>\r\n\r\n            </form>\r\n\r\n            <ul>\r\n            {newsData\r\n            .sort((a,b) => a.date < b.date ? 1 : -1)\r\n            .map((article, index) =>(\r\n                <Article \r\n                key={index} \r\n                message={article.content} \r\n                author={article.author === \"\" ? \"Anonymous\" : article.author} \r\n                date={article.date} \r\n                id={article.id}\r\n                />\r\n            ))} \r\n            </ul>\r\n\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default News;","C:\\Users\\User\\Documents\\Chinduji's doc\\Interviews prep\\React JS training\\blog-app\\src\\Components\\Header.js",[],"C:\\Users\\User\\Documents\\Chinduji's doc\\Interviews prep\\React JS training\\blog-app\\src\\Pages\\About.js",[],"C:\\Users\\User\\Documents\\Chinduji's doc\\Interviews prep\\React JS training\\blog-app\\src\\Pages\\Error.js",[],"C:\\Users\\User\\Documents\\Chinduji's doc\\Interviews prep\\React JS training\\blog-app\\src\\Components\\Navigation.js",[],"C:\\Users\\User\\Documents\\Chinduji's doc\\Interviews prep\\React JS training\\blog-app\\src\\Components\\logo.js",[],"C:\\Users\\User\\Documents\\Chinduji's doc\\Interviews prep\\React JS training\\blog-app\\src\\Components\\Article.js",[],"C:\\Users\\User\\Documents\\Chinduji's doc\\Interviews prep\\React JS training\\blog-app\\src\\Components\\DeleteArticle.js",[],"C:\\Users\\User\\Documents\\Chinduji's doc\\Interviews prep\\React JS training\\blog-app\\src\\reducer\\index.js",[],"C:\\Users\\User\\Documents\\Chinduji's doc\\Interviews prep\\React JS training\\blog-app\\src\\reducer\\Post_reducer.js",[],"C:\\Users\\User\\Documents\\Chinduji's doc\\Interviews prep\\React JS training\\blog-app\\src\\actions\\post.action.js",[],"C:\\Users\\User\\Documents\\Chinduji's doc\\Interviews prep\\React JS training\\blog-app\\src\\Components\\Utils.js