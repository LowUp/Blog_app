\n            setError(false);\r\n\r\n            }\r\n    };\r\n\r\n     useEffect(() =>{\r\n\r\n         getData();\r\n        //console.log(newsData);\r\n\r\n     }, [data])\r\n\r\n    return(\r\n        <div className=\"news-container\">\r\n            <h1>Blog</h1>\r\n\r\n            <form onSubmit={(e) => handleSubmit(e)} >\r\n\r\n                <input \r\n                type=\"text\" \r\n                placeholder=\"Name\" \r\n                value={author} \r\n                onChange={(e) => setAuthor(e.target.value)} />\r\n                \r\n                <textarea \r\n                style={error ? {border : \"1px solid red\"} : {border : \"1px solid #31dafb\"}}\r\n                placeholder=\"Message\" \r\n                value={message} onChange={(e) => setMessage(e.target.value)}/>\r\n                \r\n                { error && <p>Needs at least 20 characters</p>}\r\n\r\n                <input \r\n                type=\"submit\" \r\n                value=\"Send\"/>\r\n\r\n            </form>\r\n\r\n            <ul>\r\n            {newsData\r\n            .sort((a,b) => a.date < b.date ? 1 : -1)\r\n            .map((article, index) =>(\r\n                <Article \r\n                key={index} \r\n                message={article.content} \r\n                author={article.author === \"\" ? \"Anonymous\" : article.author} \r\n                date={article.date} \r\n                id={article.id}\r\n                />\r\n            ))} \r\n            </ul>\r\n\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default News;","C:\\Users\\User\\Documents\\Chinduji's doc\\GitHub\\Blog_app\\src\\Components\\Article.js",[],"C:\\Users\\User\\Documents\\Chinduji's doc\\GitHub\\Blog_app\\src\\Components\\DeleteArticle.js",[],{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":60,"column":9,"nodeType":"137","endLine":60,"endColumn":15,"suggestions":"138"},{"ruleId":"131","replaced